  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun ./src/db/seed.ts",
    "dev": "concurrently \"bun run --watch src/index.tsx\" \"bun run tw:dev\"",
    "tw": "bunx tailwindcss -i ./src/styles.css -o ./tailwind-gen/styles.css",
    "tw:dev": "bunx tailwindcss -i ./src/styles.css -o ./tailwind-gen/styles.css --watch",
    "lint": "bunx eslint .",
    "lint:fix": "bunx eslint --ext .ts,.tsx src --fix"
  },
  "devDependencies": {
    "bun-types": "latest",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.19.12",
    "tailwindcss": "^3.3.3",
    "typed-html": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@elysiajs/html": "^0.5.2",
    "@eslint/js": "^9.8.0",
    "@libsql/client": "^0.5.2",
    "@otherguy/elysia-logging": "^0.0.17",
    "@radix-ui/react-icons": "^1.3.0",
    "@types/eslint__js": "^8.42.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.29.4",
